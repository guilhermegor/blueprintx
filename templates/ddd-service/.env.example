# Environment variables for the service
ENV=dev
APP_NAME=ddd-service

# Available options: csv, json, sqlite, postgresql, mariadb, mysql, mssql, oracle
DB_BACKEND=json
DATA_DIR=./data
DB_FILE_JSON=records.json
DB_FILE_CSV=records.csv
SQLITE_PATH=app.db

# PostgreSQL
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=app
# Use POSTGRES_DSN to override the composed DSN above
POSTGRES_DSN=

# MariaDB
MARIADB_USER=user
MARIADB_PASSWORD=password
MARIADB_HOST=localhost
MARIADB_PORT=3306
MARIADB_DB=app
# Use MARIADB_DSN to override the composed DSN above
MARIADB_DSN=

# MySQL
MYSQL_USER=user
MYSQL_PASSWORD=password
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DB=app
# Use MYSQL_DSN to override the composed DSN above
MYSQL_DSN=

# MSSQL (SQL Server)
MSSQL_USER=sa
MSSQL_PASSWORD=password
MSSQL_HOST=localhost
MSSQL_PORT=1433
MSSQL_DB=app
# Use MSSQL_DSN to override the composed DSN above
MSSQL_DSN=

# Oracle
ORACLE_USER=user
ORACLE_PASSWORD=password
ORACLE_HOST=localhost
ORACLE_PORT=1521
ORACLE_SERVICE=XEPDB1
# Use ORACLE_DSN to override the composed DSN above
ORACLE_DSN=

# Set to true to run the CRUD demo in main.py
RUN_DEMO=false
